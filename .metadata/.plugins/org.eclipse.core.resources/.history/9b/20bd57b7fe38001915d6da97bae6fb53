package StackTest;

public class StackTest {

	/**
	 * @param args
	 */
	private int length;
	private int[] array;
	private int cord;
	public StackTest(int length){
		this.length = length;
	}
	public void put(int num){
		if(cord == length){
			System.out.println("stack was full");
		}
		this.cord = this.cord +1;
		this.array[cord] = num;
	}
	public void out(){
		this.cord = this.cord -1;
		this.array[cord] = 0;
	}

}
