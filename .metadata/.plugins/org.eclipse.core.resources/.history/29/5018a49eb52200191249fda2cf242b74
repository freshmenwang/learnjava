package Polymorphism;

public class Test01 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Animal a1 = new Cat();
		a1.move();
//		a1.catchMouse();
		Cat c1 = (Cat)a1;
		c1.catchMouse();
		Animal a2 = new Bird();
		Cat c2 = (Cat)a2;
		c2.move();
	}

}
