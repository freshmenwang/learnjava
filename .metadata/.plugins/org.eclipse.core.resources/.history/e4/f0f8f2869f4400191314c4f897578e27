package FileTest;

import java.io.File;

public class Copy {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		File f = new File("D:\\jz");
		copy(f);
	}
	public static void copy(File f){
		if(f.isFile()){
			File f1 = new File("D:/newjz" + f.getAbsolutePath().substring());
			return;
		}
	}

}
